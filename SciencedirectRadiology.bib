@article{ALNASER2023162,
title = {The impact of artificial intelligence on radiography as a profession: A narrative review},
journal = {Journal of Medical Imaging and Radiation Sciences},
volume = {54},
number = {1},
pages = {162-166},
year = {2023},
issn = {1939-8654},
doi = {https://doi.org/10.1016/j.jmir.2022.10.196},
url = {https://www.sciencedirect.com/science/article/pii/S1939865422005744},
author = {Yousif Ahmed Al-Naser},
keywords = {Artificial intelligence (AI), Deep learning, Radiography, Radiographer, Medical radiation technologist, Image acquisition, Image processing},
abstract = {Background and Purpose
Artificial intelligence (AI) algorithms, particularly deep learning, have made significant strides in image recognition and classification, providing remarkable diagnostic accuracy to various diseases. This domain of AI has been the focus of many research papers as it directly relates to the roles and responsibilities of a radiologist. However, discussions on the impact of such technology on the radiography profession are often overlooked. To address this gap in the literature, this paper aims to address the application of AI in radiography and how AI's rapid emergence into healthcare is impacting not only standard radiographic protocols but the role of the radiographic technologist as well.
Methods
A review of the literature on AI and radiography was performed, using databases within PubMed, Google Scholar, and ScienceDirect. Video presentations from YouTube were also utilized to weigh the varying opinions of world leaders at the fore of artificial intelligence.
Results
AI can augment routine standard radiographic protocols. It can automatically ensure optimal patient positioning within the gantry as well as automate image processing. As AI technologies continue to emerge in diagnostic imaging, practicing radiologic technologists are urged to achieve threshold computational and technical literacy to operate AI-driven imaging technology.
Conclusion
There are many applications of AI in radiography including acquisition and image processing. In the near future, it will be important to supply the demand for radiographers skilled in AI-driven technologies.
Résumé
Contexte et objectif
Les algorithmes d'intelligence artificielle (IA), en particulier l'apprentissage profond, ont fait des progrès considérables dans la reconnaissance et la classification d'images, offrant une précision de diagnostic remarquable pour diverses maladies. Ce domaine de l'IA a fait l'objet de nombreux documents de recherche car il est directement lié aux rôles et responsabilités d'un radiologue. Cependant, les discussions sur l'incidence de cette technologie sur la profession de radiographe sont souvent négligées. Pour combler cette lacune dans la littérature, cet article vise à aborder l'application de l'IA en radiographie et la façon dont l'émergence rapide de l'IA dans les soins de santé a une incidence non seulement sur les protocoles radiographiques standard, mais aussi sur le rôle du technologue en radiographie.
Méthodologie
Une revue de la littérature sur l'IA et la radiographie a été effectuée, en utilisant les bases de données de PubMed, Google Scholar et ScienceDirect. Des présentations vidéo de YouTube et des articles de presse ont également été utilisés pour évaluer les différentes opinions des leaders mondiaux à l'avant-garde de l'intelligence artificielle.
Résultats
L'IA peut augmenter les protocoles radiographiques standard de routine. Elle peut assurer automatiquement le positionnement optimal du patient dans le portique et automatiser le traitement des images. Comme la technologie de l'IA continue d'émerger dans le domaine de l'imagerie diagnostique, les technologues en radiologie sont invités à acquérir les connaissances informatiques et techniques nécessaires pour exploiter la technologie d'imagerie basée sur l'IA.
Conclusion
Il existe de nombreuses applications de l'IA en radiographie, notamment l'acquisition et le traitement d'images. Dans un avenir proche, il sera important de répondre à la demande de radiographes compétents dans les technologies pilotées par l'IA.}
}
@article{HONG2024596,
title = {The potential role for artificial intelligence in fracture risk prediction},
journal = {The Lancet Diabetes & Endocrinology},
volume = {12},
number = {8},
pages = {596-600},
year = {2024},
issn = {2213-8587},
doi = {https://doi.org/10.1016/S2213-8587(24)00153-0},
url = {https://www.sciencedirect.com/science/article/pii/S2213858724001530},
author = {Namki Hong and Danielle E Whittier and Claus-C Glüer and William D Leslie},
abstract = {Summary
Osteoporotic fractures are a major health challenge in older adults. Despite the availability of safe and effective therapies for osteoporosis, these therapies are underused in individuals at high risk for fracture, calling for better case-finding and fracture risk assessment strategies. Artificial intelligence (AI) and machine learning (ML) hold promise for enhancing identification of individuals at high risk for fracture by distilling useful features from high-dimensional data derived from medical records, imaging, and wearable devices. AI–ML could enable automated opportunistic screening for vertebral fractures and osteoporosis, home-based monitoring and intervention targeting lifestyle factors, and integration of multimodal features to leverage fracture prediction, ultimately aiding improved fracture risk assessment and individualised treatment. Optimism must be balanced with consideration for the explainability of AI–ML models, biases (including information inequity in numerically under-represented populations), model limitations, and net clinical benefit and workload impact. Clinical integration of AI–ML algorithms has the potential to transform osteoporosis management, offering a more personalised approach to reduce the burden of osteoporotic fractures.}
}
@article{KIM2025172,
