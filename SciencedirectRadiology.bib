doi = {https://doi.org/10.1016/j.tips.2019.06.001},
url = {https://www.sciencedirect.com/science/article/pii/S0165614719301312},
author = {Coryandar Gilvary and Neel Madhukar and Jamal Elkhader and Olivier Elemento},
keywords = {machine learning, artificial intelligence, drug development, model interpretability},
abstract = {Stakeholders across the entire healthcare chain are looking to incorporate artificial intelligence (AI) into their decision-making process. From early-stage drug discovery to clinical decision support systems, we have seen examples of how AI can improve efficiency and decrease costs. In this Opinion, we discuss some of the key factors that should be prioritized to enable the successful integration of AI across the healthcare value chain. In particular, we believe a focus on model interpretability is crucial to obtain a deeper understanding of the underlying biological mechanisms and guide further investigations. Additionally, we discuss the importance of integrating diverse types of data within any AI framework to limit bias, increase accuracy, and model the interdisciplinary nature of medicine. We believe that widespread adoption of these practices will help accelerate the continued integration of AI into our current healthcare framework.}
}
@article{FEHRMANN2024100066,
title = {How to critically appraise and direct the trajectory of AI development and application in oncology},
journal = {ESMO Real World Data and Digital Oncology},
volume = {5},
pages = {100066},
year = {2024},
issn = {2949-8201},
doi = {https://doi.org/10.1016/j.esmorw.2024.100066},
url = {https://www.sciencedirect.com/science/article/pii/S2949820124000444},
author = {R.S.N. Fehrmann and M. {van Kruchten} and E.G.E. {de Vries}},
keywords = {artificial intelligence, AI regulatory frameworks},
abstract = {As artificial intelligence (AI) advances, oncologists stand at the forefront of a transformative era in healthcare. AI, which empowers machines to learn from data, make decisions, and carry out tasks typically requiring human intelligence, is revolutionizing our clinical landscape. It promises streamlined workflows, enhanced diagnostic accuracy, and personalized treatments tailored to each patient’s unique profile. In the vast sea of patient data, AI serves as a guiding compass, ensuring no detail is overlooked, amplifying clinical acumen, and refining treatment decisions. However, to ensure AI’s benefits reach patients effectively, it is imperative that oncologists actively guide its development and application. This overview aims to equip oncologists with the tools to critically appraise and influence the trajectory of AI in oncology, ensuring its integration leads to meaningful advances in patient care.}
}
@article{JAIN2025100003,
title = {Redefining precision medicine in hepatocellular carcinoma through omics, translational, and AI-based innovations},
journal = {The Journal of Precision Medicine: Health and Disease},
volume = {1},
pages = {100003},
year = {2025},
issn = {3050-6328},
doi = {https://doi.org/10.1016/j.premed.2025.100003},
url = {https://www.sciencedirect.com/science/article/pii/S3050632825000034},
author = {Rashi Jain and Sathish Kumar Mungamuri and Prabha Garg},
keywords = {Hepatocellular carcinoma, Precision medicine, Artificial intelligence, Omics, Translational research, Biomarkers},
abstract = {Hepatocellular carcinoma (HCC) is an aggressive disease that predominantly affects patients with advanced liver ailments. HCC incidences are rapidly soaring due to challenges in early detection and resistance to conventional treatments and their associated side effects. The lack of effective biomarkers and global health disparities further complicate HCC management. This makes the advent of precision medicine all the more crucial in improving the medical care of HCC patients. This paper explores three prominent aspects of precision medicine in detail. These include omics, translational research, i.e., preclinical and clinical studies, and artificial intelligence (AI). Omics studies enable the analysis of various biological molecules to identify genetic alterations, protein expression patterns, and metabolic changes associated with HCC. They allow for the discovery of prospective biomarkers and the elucidation of underlying cancer biology mechanisms validated in preclinical studies, including animal models and cell culture experiments, before clinical assessment. AI can revolutionize HCC management by integrating and analyzing vast amounts of data from omics, preclinical, and clinical research. AI-powered tools provide a multitude of applications to improve patient outcomes. Together, these facets culminate in a comprehensive precision medicine approach that can be implemented to transform the landscape of HCC.}
}
@article{SAFDAR2020108768,
