pages = {100309},
year = {2025},
issn = {2949-8295},
doi = {https://doi.org/10.1016/j.nxnano.2025.100309},
url = {https://www.sciencedirect.com/science/article/pii/S2949829525001780},
author = {Anugya Arya and Dhruv Garg and Amit Parashar and Harish Chandra Joshi and Waseem Ahmad},
keywords = {Artificial intelligence, Nanotechnology, Energy conversion, Biomaterial design},
abstract = {Artificial intelligence and nanotechnology hold remarkable potential to reshape scientific research, technological innovation, and daily life. Nanotechnology is dedicated to the manipulation of materials at the nanoscale, and artificial intelligence is focused on developing systems that can perform tasks autonomously. The merging of nanotechnology and artificial intelligence is unlocking new and powerful ways to elevate the power and potential of intelligent systems. This review investigates the blending of AI and nanotechnology, with a focus on emerging trends, existing challenges, and potential prospects across diverse fields. A thorough in-depth study of the current investigation and implementation has revealed key patterns, obstacles, and future opportunities for research and applications. This review highlights the profound influence of AI in guiding nanotechnology across various fields such as health care, environmental monitoring, energy conversion, and quantum computing.}
}
@article{ADAMIDI20212833,
title = {Artificial intelligence in clinical care amidst COVID-19 pandemic: A systematic review},
journal = {Computational and Structural Biotechnology Journal},
