@article{FAJKISZAJACZKOWSKA2025110750,
title = {Comprehensive analysis of frameworks for evaluating artificial intelligence solutions in Healthcare: A descriptive review},
journal = {Computers in Biology and Medicine},
volume = {196},
pages = {110750},
year = {2025},
issn = {0010-4825},
doi = {https://doi.org/10.1016/j.compbiomed.2025.110750},
url = {https://www.sciencedirect.com/science/article/pii/S0010482525011011},
author = {Nikola Fajkis-Zajączkowska and Anna Zawada and Magdalena Bojko and Katarzyna Kolasa},
keywords = {Artificial intelligence, Healthcare, Health technology assessment, Medical device},
abstract = {Background
Despite the lack of pricing and reimbursement guidelines for AI driven health technologies at majority of EU jurisdictions, there are some new examples of value assessment framework for digital solutions. To facilitate the process of homogenous guidelines formulation for those EU countries without any guidelines there is a need to review the current state of art regarding the assessment criteria proposed by various research groups. This will not only facilitate an understanding of the usability of available frameworks but also aid decision-makers in composing future guidelines for value of AI-based medical technologies assessment. Given the pervasive impact of the digital revolution and the resulting development of AI-based technologies in medicine, as well as emerging frameworks for their evaluation, there is a critical need for a descriptive analysis of existing frameworks, which is the main goal of this article.
Method
A literature review was performed in accordance with recommendations in the Preferred Reporting Items for Systematic Reviews and Meta-Analyses (PRISMA) guidelines. Based on 2649 hits, 11 studies (13 articles) were selected for descriptive analysis. The evaluation criteria for AI solutions along with definitions extracted from the included articles were aggregated using a language model – ChatGPT (ver. 3.5) – to maintain objectivity in the process.
Results
The result of our analysis is an aggregated framework comprising five domains (Clinical Assessment, Economics, Ethics, Safety, and Usability), within which 35 distinct criteria for evaluating AI products, along with their definitions, have been established.
Conclusions
According to the authors' knowledge, this framework is the most comprehensive, encompassing such a wide range of aspects. With this comprehensive approach, decision-makers can directly compare various technologies addressing the same problem, making informed decisions about which solution best meets their needs and requirements. In addition to its comprehensiveness, an important feature of our aggregated framework is that it can draw an insight to payers, healthcare providers, and technical teams.}
}
@article{MANIMARAN2025111324,
title = {Explainable deep learning based techniques for ECG-Based heart disease classification: A systematic literature review and future direction},
journal = {Computers in Biology and Medicine},
volume = {199},
pages = {111324},
year = {2025},
issn = {0010-4825},
doi = {https://doi.org/10.1016/j.compbiomed.2025.111324},
url = {https://www.sciencedirect.com/science/article/pii/S0010482525016786},
author = {Gouthamaan Manimaran and Abdolrahman Peimankar and Sadasivan Puthusserypady and Mahdi Momeni and Rifa Atul Izza Asyari and Masud Shah Jahan and Jonas Moll and Uffe Kock Wiil and Ali Ebrahimi},
keywords = {Heart disease, Deep learning, Explainable AI, ECG, Classification, Systematic literature review},
abstract = {Objective
This study aims to improve the understanding of explainability in deep learning (DL) architectures utilized for heart disease (HD) classification through electrocardiogram (ECG) data. It offers a systematic review of methodological choices, analyses their impacts on model interpretability, and highlights significant challenges in this domain while suggesting opportunities for future research.
Methods
A systematic literature review (SLR) was conducted based on Kitchenham and Charters guidelines. To address the proposed research questions, this article provides an SLR of academic articles on Explainable AI (XAI)-based DL for the classification of HDs using ECGs dated from January 2018 to September 2024. Results were synthesized by identifying the valuable insights into the datasets, preprocessing methods, and techniques employed in the development of XAI-based DL models for the classification of ECG-based HDs.
Results
This study identified 6448 primary studies that utilized machine learning and DL techniques for the classification of HD based on ECG data, of which 51 used an XAI-based DL architecture. Our finding indicates that there are 25 different datasets that were used, 16 different DL architectures were introduced, and eight novel XAI techniques were introduced, while most of the selected studies used the conventional XAI approaches such as SHAP, Saliency Maps, Grad-Cam, and LIME.
Conclusions
A considerable amount of XAI-based DL architectures were identified for the classification of ECG-based HD. The techniques frequently selected by these studies have potential limitations such as data standardization, inconsistent explainability, temporal dependency visualization, lack of XAI benchmarking, lack of standardized metrics, and many more, which are presented with proposed future research directions.}
}
@article{ZUBAIR2024103934,
