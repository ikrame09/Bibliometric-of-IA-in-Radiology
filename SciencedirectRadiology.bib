volume = {122},
pages = {108768},
year = {2020},
issn = {0720-048X},
doi = {https://doi.org/10.1016/j.ejrad.2019.108768},
url = {https://www.sciencedirect.com/science/article/pii/S0720048X19304188},
author = {Nabile M. Safdar and John D. Banja and Carolyn C. Meltzer},
keywords = {Artificial intelligence, Machine learning, Ethics, Radiology},
abstract = {With artificial intelligence (AI) precipitously perched at the apex of the hype curve, the promise of transforming the disparate fields of healthcare, finance, journalism, and security and law enforcement, among others, is enormous. For healthcare – particularly radiology – AI is anticipated to facilitate improved diagnostics, workflow, and therapeutic planning and monitoring. And, while it is also causing some trepidation among radiologists regarding its uncertain impact on the demand and training of our current and future workforce, most of us welcome the potential to harness AI for transformative improvements in our ability to diagnose disease more accurately and earlier in the populations we serve.}
}
@article{MADRIDGARCIA2023152213,
