@article{LEWIN20241897,
title = {Ethical Challenges and Opportunities in Applying Artificial Intelligence to Cardiovascular Medicine},
journal = {Canadian Journal of Cardiology},
volume = {40},
number = {10},
pages = {1897-1906},
year = {2024},
note = {Theme Issue: Rapidly Evolving Clinical Applications of Artificial Intelligence},
issn = {0828-282X},
doi = {https://doi.org/10.1016/j.cjca.2024.06.029},
url = {https://www.sciencedirect.com/science/article/pii/S0828282X24005671},
author = {Stephen Lewin and Riti Chetty and Abdul Rahman Ihdayhid and Girish Dwivedi},
abstract = {Much anticipation surrounds artificial intelligence’s (AI) emergence as a promising tool in health care. It offers potential to revolutionise clinical practice through assistive and autonomous operation. The high prevalence of cardiac disease globally provides an opportunity for AI technology to increase health care efficiency and improve patient outcomes. This article explores the ethical considerations necessary for safe and acceptable implantation of AI within the health care space. We aim to highlight several challenges such as data privacy, consent, sustainability, and cybersecurity. In addition, we outline the future opportunities for AI use in cardiovascular medicine. Overall, we argue that AI deployment demands robust regulation, transparent algorithms, and safeguarding of patient privacy.
Résumé
Il y a beaucoup d'attentes autour de l'émergence de l'intelligence artificielle (IA) comme outil prometteur dans le domaine des soins de santé. Elle offre le potentiel de révolutionner la pratique clinique grâce à des opérations d'assistance et d'autonomie. La forte prévalence des maladies cardiaques à l'échelle mondiale offre une opportunité pour la technologie de l'IA d'accroître l'efficacité des soins de santé et d'améliorer les pronostics pour les patients. Cet article explore les considérations éthiques nécessaires pour une implantation sûre et acceptable de l'IA dans l'espace des soins de santé. Nous visons à mettre en lumière plusieurs défis tels que la protection des données, le consentement, la durabilité et la cybersécurité. De plus, nous décrivons les opportunités futures pour l'utilisation de l'IA en médecine cardiovasculaire. Dans l'ensemble, nous soutenons que le déploiement de l'IA exige une réglementation robuste, des algorithmes transparents et la protection de la vie privée des patients.}
}
@article{ALABI2021102060,
title = {Machine learning in oral squamous cell carcinoma: Current status, clinical concerns and prospects for future—A systematic review},
journal = {Artificial Intelligence in Medicine},
