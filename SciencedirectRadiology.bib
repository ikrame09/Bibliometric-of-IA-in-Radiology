@article{VASEY2023130,
title = {DECIDE-AI: a new reporting guideline and its relevance to artificial intelligence studies in radiology},
journal = {Clinical Radiology},
volume = {78},
number = {2},
pages = {130-136},
year = {2023},
note = {Special Issue Section: Artificial Intelligence and Machine Learning},
issn = {0009-9260},
doi = {https://doi.org/10.1016/j.crad.2022.09.131},
